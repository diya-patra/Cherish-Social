{
    "version": 2,
    "builds": [
        {
            "src": "backend/vercel.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/vercel.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/$1"
        }
    ]
}